type User {
  id: ID! @id
  email: String @unique
  firstName: String!
  lastName: String!
  password: String!
  admin: Boolean!
  checkins: [Checkin!]!
  friends: [User!]!
  }

type Product {
  id: ID! @id
  name: String!
  producer: String
  type: String
  checkins: [Checkin!]!
  }

type Checkin {
    id: ID! @id
    rating: Int!
    comment: String!
    author: User!
    product: Product!
    createdAt: DateTime! @createdAt
    updatedAt: DateTime! @updatedAt
}